package __basic

import "context"

@hmlt basic() {
%p one & done
}

@hmlt basic2(fizz string, list map[string]string) {
	%p#fizz.foo{@attributes:{list}}
		indented one & done
	%img{src: "b&ar.png", title: {fizz}}
}

@hmlt basic3(fizz string) {
!!! 5
%html
	%head
		%title= fizz
	%body
		%p! #{fizz} buzz
}

@hmlt basic4(items []string) {
%ul
	- for _, item := range items
		%li= item
}
